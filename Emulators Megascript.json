{
	"info": {
		"title": "Emulators Megascript",
		"author": "Glazed_Belmont",
		"shortDesc": "Includes downloads for emulators.",
		"description": "With this script, you can download and update\nthe following emulators: \n- mGBA\n- SNES9X\n- VirtuaNES\n- PicoDrive\n- Neopop\n- GameYob (3DS)\n- GameYob (DSi)\nScript by Glazed_Belmont",
		"barColor": "#395472",
		"bgBottomColor": "#262C4D",
		"bgTopColor": "#60A8C0",
		"textColor": "#FFFFFF",
		"selectedColor": "#78C0D8",
		"unselectedColor": "#4D7684",
		"version": 2,
		"revision": 2
	},
	"Gameyob 3DS 3DSX": [
		{
			"type": "downloadFile",
			"file": "https://github.com/Steveice10/GameYob/releases/download/1.0.8/GameYob.zip",
			"output": "/3ds/GameYob.zip",
			"message": "Downloading the GameYob Zip..."
		},
		{
            "type": "extractFile",
            "file": "/3ds/GameYob.zip",
            "input": "GameYob/3ds-arm/3ds/GameYob/",
            "output": "/3ds/GameYob.3dsx",
			"message": "Extract GameYob..."
		},
		{
			"type": "deleteFile",
			"file": "/3ds/GameYob.zip"
		}
		
	],
